{
  "manifest_version": 2,
  "name": "purpose",
  "version": "1.0",
  "description": "Stay focused by being reminded of your purpose on certain websites.",
  "homepage_url": "https://github.com/dkarkada/purpose",
  "icons": {
    "48": "icons/chillout-48.png" //WRONG
  },

  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  
  "browser_action": {
      "default_title": "Click Me"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "web_accessible_resources": [
    "askPurpose.js"
  ]

}